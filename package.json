{
  "name": "react-teller",
  "version": "0.0.0-beta.1",
  "description": "ReactJS notifications for ES6 age",
  "main": "./dist/react-teller.js",
  "scripts": {
    "build": "npm run clean && webpack --config webpack.dist.config.js",
    "clean": "rimraf ./dist",
    "start": "webpack-dev-server",
    "test": "NODE_ENV=test nyc ava --verbose",
    "test:full": "nyc ava && nyc report --reporter=text-lcov | coveralls",
    "format": "prettier --write 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yatharthk/react-teller.git"
  },
  "keywords": [
    "react",
    "react-component",
    "react-notifications",
    "notifications",
    "messages",
    "animate",
    "es6",
    "template-literals",
    "templates",
    "config",
    "prettier-formatted"
  ],
  "author": "Yatharth Khatri <yatharth01@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yatharthk/react-teller/issues"
  },
  "homepage": "https://github.com/yatharthk/react-teller#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "ava": "^0.17.0",
    "babel-core": "^6.7.4",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.22.0",
    "compression-webpack-plugin": "^0.4.0",
    "coveralls": "^2.11.15",
    "css-loader": "^0.28.0",
    "enzyme": "^2.7.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "json-loader": "^0.5.4",
    "nyc": "^10.1.2",
    "postcss-loader": "^1.3.3",
    "prettier": "^1.2.0",
    "react": "^15.0.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.0.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "2.x",
    "webpack-dev-server": "1.11.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
